<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.box.small.supportMapper">
	
	<select id="faq" resultType="com.box.small.dto.Support">
		select * from board
		where cat_no = 2
	</select>
	
	<select id="selectFaq" parameterType="int" resultType="com.box.small.dto.Support">
		select * from board
		where bo_no=#{bo_no}
	</select>
   	
   	
</mapper>